{
  "name": "rougemarketapi",
  "version": "0.1.2",
  "description": "RougeMarket API (UDF-Compatible)",
  "main": "src/app.js",
  "scripts": {
    "clean": "rm -rf build/ && rm -rf ./node_modules",
    "pre_build": "npm install",
    "build-swagger": "tsoa spec ",
    "build-routes": "tsoa routes ",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build": "tsoa spec-and-routes && tsc --build && ts-project-bundle --out=build",
    "start": "node dist/server.js"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node": "^16.3.3",
    "ts-node": "^10.8.1",
    "ts-project-bundle": "0.0.8",
    "@redis/json": "^1.0.4",
    "@types/multer": "1.4.7",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-redis-cache": "^1.1.3",
    "redis": "^4.3.1",
    "swagger-ui-express": "^4.5.0",
    "tsoa": "^4.1.1"
  },
  "devDependencies": {
    "dayjs": "^1.10.6",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/multer": "1.4.7",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
